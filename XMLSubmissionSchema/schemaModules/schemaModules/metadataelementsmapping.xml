<?xml version="1.0"?>
<mapping>
	<class name="uk.co.gazettes.services.search.castor.entities.MetadataElements">
		<map-to xml="get-metadata-elements" />

		<field name="edition" type="string">
			<bind-xml name="edition" node="element" />
		</field>
		<field name="issueNumber" type="string">
			<bind-xml name="issue" node="element" />
		</field>
		<field name="type" type="string">
			<bind-xml name="type" node="element" />
		</field>
		<field name="contentType" type="string">
			<bind-xml name="content-type" node="element" />
		</field>
		<field name="pageNumber" type="string">
			<bind-xml name="page-number" node="element" />
		</field>
		<field name="status" type="string">
			<bind-xml name="status" node="element" />
		</field>
		<field name="publicationDate" type="string">
			<bind-xml name="publication-date" node="element" />
		</field>
		<field name="pageNumberStart" type="string">
			<bind-xml name="page-number-start" node="element" />
		</field>
		<field name="pageNumberEnd" type="string">
			<bind-xml name="page-number-end" node="element" />
		</field>
		<field name="totalPages" type="string">
			<bind-xml name="total-pages" node="element" />
		</field>
		<field name="noticeCode" type="string">
			<bind-xml name="notice-code" node="element" />
		</field>
		<field name="draftUri" type="string">
			<bind-xml name="draft-uri" node="element" />
		</field>
		<field name="noticeId" type="string">
			<bind-xml name="notice-id" node="element" />
		</field>
		<field name="title" type="string">
			<bind-xml name="title" node="element" />
		</field>
		<field name="noticeCaptureMethod" type="string">
			<bind-xml name="notice-capture-method" node="element" />
		</field>
	</class>

</mapping>