<?xml version="1.0" encoding="UTF-8"?>
<mapping>
	<class name="uk.co.gazettes.services.tsoshop.castor.entities.productservice.GetProductResponse">	  
	    <map-to xml="GetProductResponse" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos" />	      
		<field name="result" type="uk.co.gazettes.services.tsoshop.castor.entities.productservice.GetProductResult" >
			<bind-xml name="GetProductResult" node="element" />
		</field> 
	</class> 
	
	<class name="uk.co.gazettes.services.tsoshop.castor.entities.productservice.GetProductResult">	  
	    <map-to xml="GetProductResult" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos"/>	      
		<field name="productId" type="string" >
			<bind-xml name="ProductId" node="element" />
		</field>
		<field name="productName" type="string" >
			<bind-xml name="ProductName" node="element" />
		</field>
		<field name="productType" type="string" >
			<bind-xml name="ProductType" node="element" />
		</field>  
		<field name="briefDescription" type="string" >
			<bind-xml name="BriefDescription" node="element" />
		</field> 
		<field name="fullDescription" type="string" >
			<bind-xml name="FullDescription" node="element" />
		</field> 
		<field name="lastModifiedDate" type="date" >
			<bind-xml name="LastModifiedDate" node="element" />
		</field> 
		<field name="maxOrderQuantity" type="int" >
			<bind-xml name="MaxOrderQuantity" node="element" />
		</field>
		<field name="minOrderQuantity" type="int" >
			<bind-xml name="MinOrderQuantity" node="element" />
		</field>
	    <field name="unitPrices" type="uk.co.gazettes.services.tsoshop.castor.entities.productservice.UnitPrices" >
	      	<bind-xml name="UnitPrices" node="element"/>
	    </field>
	    <field name="vatCode" type="uk.co.gazettes.services.tsoshop.castor.entities.productservice.VatCode" >
	      	<bind-xml name="VatCode" node="element"/>
	    </field>
	</class> 
	
	<class name="uk.co.gazettes.services.tsoshop.castor.entities.productservice.VatPercentConfigs">
	  	<map-to xml="VatPercentConfigs" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos" />	      
 		<field name="vatPercentConfigList" type="uk.co.gazettes.services.tsoshop.castor.entities.productservice.VatPercentConfig" collection="arraylist">
	      	<bind-xml name="VatPercentConfig" node="element"/>
	    </field>
	</class>
	
	<class name="uk.co.gazettes.services.tsoshop.castor.entities.productservice.VatPercentConfig">
	  	<map-to xml="VatPercentConfig" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos" />	      
 		<field name="vatCodeId" type="string" >
	      	<bind-xml name="VatCodeId" node="element"/>
	    </field>
	    <field name="b2BVatPercent" type="string" >
	      	<bind-xml name="B2BVatPercent" node="element"/>
	    </field>
	     <field name="b2CVatPercent" type="string" >
	      	<bind-xml name="B2CVatPercent" node="element"/>
	    </field>
	</class>
	
	<class name="uk.co.gazettes.services.tsoshop.castor.entities.productservice.UnitPrices">
	  	<map-to xml="UnitPrices" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos" />	      
 		<field name="unitPricesList" type="uk.co.gazettes.services.tsoshop.castor.entities.productservice.UnitPrice" collection="arraylist">
	      	<bind-xml name="UnitPrice" node="element"/>
	    </field>
	</class>

	<class name="uk.co.gazettes.services.tsoshop.castor.entities.productservice.UnitPrice">
	  	<map-to xml="UnitPrice" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos" />	      
 		<field name="breakQuantity" type="int" >
	      	<bind-xml name="BreakQuantity" node="element"/>
	    </field>
	    <field name="currencyCode" type="string" >
	      	<bind-xml name="CurrencyCode" node="element"/>
	    </field>
	     <field name="netPrice" type="string" >
	      	<bind-xml name="NetPrice" node="element"/>
	    </field>
	    <field name="validFromDate" type="date" >
	      	<bind-xml name="ValidFromDate" node="element"/>
	    </field>
	    <field name="validToDate" type="date" >
	      	<bind-xml name="ValidToDate" node="element"/>
	    </field>
	</class>
	
	<class name="uk.co.gazettes.services.tsoshop.castor.entities.productservice.VatCode">	  
	    <map-to xml="VatCode" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos" />	      
		<field name="vatCodeId" type="string" >
	      	<bind-xml name="VatCodeId" node="element"/>
	    </field>
		<field name="vatRatePercent" type="string" >
	      	<bind-xml name="VatRatePercent" node="element"/>
	    </field>
	    <field name="vatPercentConfigs" type="uk.co.gazettes.services.tsoshop.castor.entities.productservice.VatPercentConfigs" >
	      	<bind-xml name="VatPercentConfigs" node="element"/>
	    </field>
	</class>
	
</mapping>