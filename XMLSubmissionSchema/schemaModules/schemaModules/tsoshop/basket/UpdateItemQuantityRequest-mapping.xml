<?xml version="1.0"?>
<mapping>
 	<class name="uk.co.gazettes.services.tsoshop.castor.entities.basketservice.UpdateItemQuantityRequest">	  
	    <map-to xml="UpdateItemQuantity" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos" />	      
	    <field name="customerId" type="string" >
	      	<bind-xml name="customerId" node="element"/>
	    </field> 
	    <field name="basketId" type="string" >
	      	<bind-xml name="basketId" node="element"/>
	    </field>
	    <field name="basketItemId" type="string" >
	      	<bind-xml name="basketItemId" node="element"/>
	    </field> 
	    <field name="quantity" type="int" >
	      	<bind-xml name="quantity" node="element"/>
	    </field> 	    	            
	</class>
</mapping>