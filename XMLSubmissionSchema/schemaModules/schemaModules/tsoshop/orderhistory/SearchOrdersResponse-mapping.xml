	    </field> 
	    <field name="totalQuantities" type="string" >
	      	<bind-xml name="TotalQuantities" node="element"/>
	    </field> 
	</class>  
	<class name="uk.co.gazettes.services.tsoshop.castor.entities.orderhistoryservice.GetOrderOfActiveSubscriptionResponse">	  
	    <map-to xml="GetOrderOfActiveSubscriptionResponse" ns-uri="http://tempuri.org/" />	      
		<field name="result" type="uk.co.gazettes.services.tsoshop.castor.entities.orderhistoryservice.GetOrderOfActiveSubscriptionResult" >
			<bind-xml name="GetOrderOfActiveSubscriptionResult" node="element" />
		</field> 
	</class>
	
