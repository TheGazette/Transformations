	      	<bind-xml name="Order" node="element"/>
	    </field>
	</class>
	
	<class name="uk.co.gazettes.services.tsoshop.castor.entities.orderhistoryservice.Order">	  
	    <map-to xml="Order" ns-uri="http://tempuri.org/" ns-prefix="temp" />	      
