	    </field>
		<field name="billingTypeId" type="string" >
	      	<bind-xml name="BillingTypeId" node="element"/>
	    </field>
	    <field name="buyerPONumber" type="string" >
	      	<bind-xml name="BuyerPONumber" node="element"/>
