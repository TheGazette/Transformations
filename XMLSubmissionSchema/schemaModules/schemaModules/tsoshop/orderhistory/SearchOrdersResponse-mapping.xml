<?xml version="1.0" encoding="UTF-8"?>
<mapping>
	<class name="uk.co.gazettes.services.tsoshop.castor.entities.orderhistoryservice.SearchOrdersResponse">	  
	    <map-to xml="SearchOrdersResponse" ns-uri="http://tempuri.org/" ns-prefix="temp" />	      
		<field name="result" type="uk.co.gazettes.services.tsoshop.castor.entities.orderhistoryservice.SearchOrdersResult" >
			<bind-xml name="SearchOrdersResult" node="element" />
		</field> 
	</class> 
	
	
	
	<class name="uk.co.gazettes.services.tsoshop.castor.entities.orderhistoryservice.SearchOrdersResult">	  
	    <map-to xml="SearchOrdersResult" ns-uri="http://tempuri.org/" ns-prefix="temp" />	      
		<field name="itemsPerPage" type="string" >
	      	<bind-xml name="ItemsPerPage" node="element"/>
	    </field>
		<field name="pageItems" type="uk.co.gazettes.services.tsoshop.castor.entities.orderhistoryservice.PageItems" >
	      	<bind-xml name="PageItems" node="element"/>
	    </field>
	    <field name="pageNumber" type="string" >
	      	<bind-xml name="PageNumber" node="element"/>
	    </field>
	    <field name="totalItems" type="string" >
	      	<bind-xml name="TotalItems" node="element"/>
	    </field>  
	    <field name="totalPages" type="string" >
	      	<bind-xml name="TotalPages" node="element"/>
	    </field> 
	</class>
	
	<class name="uk.co.gazettes.services.tsoshop.castor.entities.orderhistoryservice.PageItems">	  
	    <map-to xml="PageItems" ns-uri="http://tempuri.org/" ns-prefix="temp" />	      
