	    </field>
	    <field name="orderPaymentType" type="string" >
	      	<bind-xml name="OrderPaymentType" node="element"/>
	    </field>
		<field name="orderTotal" type="string" >
	      	<bind-xml name="OrderTotal" node="element"/>
	    </field>
	    <field name="orderTotalCharges" type="string" >
	      	<bind-xml name="OrderTotalCharges" node="element"/>
	    </field>
	    <field name="orderTotalChargesVAT" type="string" >
