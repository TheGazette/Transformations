<?xml version="1.0"?>
<mapping>
	<class name="uk.co.gazettes.services.tsoshop.castor.entities.common.OperationResult">	  
	    <map-to xml="OperationResult" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsoshop" />	      
		<field name="basketId" type="string" >
	      	<bind-xml name="BasketId" node="element"/>
	    </field>
		<field name="createDate" type="date" >
	      	<bind-xml name="CreateDate" node="element"/>
	    </field>
	    <field name="customerId" type="string" >
	      	<bind-xml name="CustomerId" node="element"/>
	    </field>
	    <field name="lineItemId" type="string" >
	      	<bind-xml name="LineItemId" node="element"/>
	    </field>
		<field name="lineVATValue" type="string" >
	      	<bind-xml name="LineVATValue" node="element"/>
	    </field>
	    <field name="lineValue" type="string" >
	      	<bind-xml name="LineValue" node="element"/>
	    </field>
	    <field name="orderedQuantity" type="int" >
	      	<bind-xml name="OrderedQuantity" node="element"/>
	    </field>
	    <field name="orderedQuantityIsValid" type="boolean" >
	      	<bind-xml name="OrderedQuantityIsValid" node="element"/>
	    </field>
	    <field name="unitNetPrice" type="string" >
	      	<bind-xml name="UnitNetPrice" node="element"/>
	    </field>
	    <field name="lastModifiedDate" type="date" >
	      	<bind-xml name="LastModifiedDate" node="element"/>
	    </field>
	</class>
</mapping>