	<xsd:element name="Text">
		<xsd:annotation>
			<xsd:documentation>
			    A container for a line of text - for CLS Text should contain either Authority AND Administration or LegislationSection.			
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
		    <xsd:choice minOccurs="0">
			    <xsd:sequence>			        
			        <xsd:element ref="Authority"/>
			        <xsd:element ref="Administration"/>	
			    </xsd:sequence>
	            <xsd:element ref="LegislationSection" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	
</xsd:schema>
