  elementFormDefault="qualified"
  version="1.1">

  <xs:element name="relations">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="relation" type="xs:string" maxOccurs="unbounded" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

</xs:schema>

