<?xml version="1.0"?>
<mapping>
	<class name="uk.co.gazettes.services.tsoshop.castor.entities.common.ValidationMessages">
	  	<map-to xml="ValidationMessages" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos" />	      
 		<field name="validationMessages" type="uk.co.gazettes.services.tsoshop.castor.entities.common.ValidationMessage" collection="arraylist" >
	      	<bind-xml name="ValidationMessage" node="element"/>
	    </field>
	</class>
	
	<class name="uk.co.gazettes.services.tsoshop.castor.entities.common.ValidationMessage">
	  	<map-to xml="ValidationMessage" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos" />	      
 		<field name="lineItemId" type="string" >
	      	<bind-xml name="LineItemId" node="element"/>
	    </field>
	    <field name="messageResourceId" type="string" >
	      	<bind-xml name="MessageResourceId" node="element"/>
	    </field>
	    <field name="notificationLevel" type="string" >
	      	<bind-xml name="NotificationLevel" node="element"/>
	    </field>
	    <field name="productId" type="string" >
	      	<bind-xml name="ProductId" node="element"/>
	    </field>
	</class>
</mapping>