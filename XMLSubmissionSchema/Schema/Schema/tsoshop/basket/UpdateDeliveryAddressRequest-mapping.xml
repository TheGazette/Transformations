<?xml version="1.0" encoding="UTF-8"?>
<mapping>
 	<class name="uk.co.gazettes.services.tsoshop.castor.entities.basketservice.UpdateDeliveryAddressRequest">	  
	    <map-to xml="UpdateDeliveryAddress" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos" />	      
	    <field name="customerId" type="string" >
	      	<bind-xml name="customerId" node="element"/>
	    </field> 	
	    <field name="basketId" type="string" >
	      	<bind-xml name="basketId" node="element"/>
	    </field>     	            
	    <field name="addressId" type="string" >
	      	<bind-xml name="addressId" node="element"/>
	    </field>    
	    <field name="applyToAllItems" type="boolean" >
	      	<bind-xml name="applyToAllItems" node="element"/>
	    </field>    
	</class>
</mapping>