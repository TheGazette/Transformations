<?xml version="1.0" encoding="UTF-8"?>
<mapping>
    <class name="uk.co.gazettes.services.tsoshop.castor.entities.productservice.GetProductsTotalPriceRequest">
        <map-to xml="GetProductsTotalPrice" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos"/>
        <field name="productsPriceRequests" type="uk.co.gazettes.services.tsoshop.castor.entities.productservice.GetProductsPriceRequests">
            <bind-xml name="ProductsPriceRequests" node="element"/>
        </field>
    </class>

    <class name="uk.co.gazettes.services.tsoshop.castor.entities.productservice.GetProductsPriceRequests">
        <field name="productsPriceRequest" type="uk.co.gazettes.services.tsoshop.castor.entities.productservice.GetProductTotalRequestModel" collection="arraylist">
            <bind-xml name="ProductsPriceRequest" node="element"/>
        </field>
    </class>

    <class name="uk.co.gazettes.services.tsoshop.castor.entities.productservice.GetProductTotalRequestModel">
        <map-to xml="ProductsPriceRequest" ns-uri="http://schemas.datacontract.org/2004/07/Tso.Shop.Model.WebService" ns-prefix="tso"/>
        <field name="productId" type="string">
            <bind-xml name="ProductId" node="element"/>
        </field>
        <field name="quantity" type="integer">
            <bind-xml name="Quantity" node="element"/>
        </field>
    </class>
</mapping>