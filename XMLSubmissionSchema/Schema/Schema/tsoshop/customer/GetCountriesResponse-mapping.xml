<?xml version="1.0"?>
<mapping>
 	<class name="uk.co.gazettes.services.tsoshop.castor.entities.customerservice.GetCountriesResponse">	  
	    <map-to xml="GetCountriesResponse" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos" />	      
	    <field name="result" type="uk.co.gazettes.services.tsoshop.castor.entities.customerservice.GetCountriesResult" >
	      	<bind-xml name="GetCountriesResult" node="element"/>
	    </field> 	    	            
	</class>
	  
	<class name="uk.co.gazettes.services.tsoshop.castor.entities.customerservice.GetCountriesResult">	  
	    <map-to xml="GetCountriesResult" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos" />	      
		<field name="countryList" type="uk.co.gazettes.services.tsoshop.castor.entities.common.Country" collection="arraylist" >
			<bind-xml name="Country" node="element" />
		</field> 
	</class>  
	
	<class name="uk.co.gazettes.services.tsoshop.castor.entities.common.Country">	  
	    <map-to xml="Country" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos" />	      
		<field name="countryCode" type="string" >
			<bind-xml name="CountryCode" node="element" />
		</field>
		<field name="countryName" type="string" >
			<bind-xml name="CountryName" node="element" />
		</field> 
	</class>  
</mapping>