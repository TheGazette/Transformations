<?xml version="1.0"?>
<mapping>
 	<class name="uk.co.gazettes.services.tsoshop.castor.entities.customerservice.GetCustomerByIdResponse">	  
	    <map-to xml="GetCustomerResponse" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos" />	      
	    <field name="result" type="uk.co.gazettes.services.tsoshop.castor.entities.customerservice.CustomerByIdResult" >
	      	<bind-xml name="GetCustomerResult" node="element"/>
	    </field> 	    	            
	</class>
	  
	<class name="uk.co.gazettes.services.tsoshop.castor.entities.customerservice.CustomerByIdResult">	  
	    <map-to xml="GetCustomerResult" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos" />	      
		<field name="title" type="string">
			<bind-xml name="Title" node="element" />
		</field>
		<field name="firstName" type="string">
			<bind-xml name="FirstName" node="element" />
		</field>
		<field name="email" type="string">
			<bind-xml name="Email" node="element" />
		</field>
		<field name="jobTitle" type="string">
			<bind-xml name="JobTitle" node="element" />
		</field>
		<field name="middleName" type="string">
			<bind-xml name="MiddleName" node="element" />
		</field>
		<field name="surname" type="string">
			<bind-xml name="Surname" node="element" />
		</field>
		<field name="telephone" type="string">
			<bind-xml name="Telephone" node="element" />
		</field>
		<field name="isApproved" type="boolean">
			<bind-xml name="IsApproved" node="element" />
		</field>
		<field name="organisation" type="string">
			<bind-xml name="Organisation" node="element" />
		</field>
		<field name="organisationId" type="string">
			<bind-xml name="OrganisationId" node="element" />
		</field>
		<field name="customerId" type="string">
			<bind-xml name="CustomerId" node="element" />
		</field>
		<field name="addresses" type="uk.co.gazettes.services.tsoshop.castor.entities.customerservice.Addresses">
			<bind-xml name="Addresses" node="element" />
		</field>
		<field name="billToAddress" type="uk.co.gazettes.services.tsoshop.castor.entities.customerservice.BillToAddress">
			<bind-xml name="BillToAddress" node="element" />
		</field>
		<field name="defaultShipToAddress" type="uk.co.gazettes.services.tsoshop.castor.entities.customerservice.DefaultShipToAddress">
			<bind-xml name="DefaultShipToAddress" node="element" />
		</field>
		<field name="groups" type="uk.co.gazettes.services.tsoshop.castor.entities.customerservice.Groups">
			<bind-xml name="Groups" node="element" />
		</field>
	</class> 
	
	<class name="uk.co.gazettes.services.tsoshop.castor.entities.customerservice.Groups">
	  	<map-to xml="Groups" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos" />
	  	<field name="customerGroups" type="uk.co.gazettes.services.tsoshop.castor.entities.customerservice.CustomerGroup" collection="arraylist">
			<bind-xml name="CustomerGroup" node="element" />
		</field>
	</class> 
	
	<class name="uk.co.gazettes.services.tsoshop.castor.entities.customerservice.CustomerGroup">
	  	<map-to xml="CustomerGroup" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos" />
	  	<field name="groupId" type="string">
			<bind-xml name="GroupId" node="element" />
		</field>
		<field name="groupName" type="string">
			<bind-xml name="GroupName" node="element" />
		</field>
	</class> 
	
</mapping>