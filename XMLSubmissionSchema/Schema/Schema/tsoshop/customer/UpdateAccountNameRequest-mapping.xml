<?xml version="1.0"?>
<mapping>
 	<class name="uk.co.gazettes.services.tsoshop.castor.entities.customerservice.UpdateAccountNameRequest">	  
	    <map-to xml="UpdateAccountName" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos" />	      
	    <field name="customerId" type="string" >
	      	<bind-xml name="customerId" node="element"/>
	    </field>
	    <field name="firstName" type="string" >
	      	<bind-xml name="firstName" node="element" />
	    </field>
	    <field name="surname" type="string" >
	      	<bind-xml name="surname" node="element" />
	    </field>
	</class>
</mapping>