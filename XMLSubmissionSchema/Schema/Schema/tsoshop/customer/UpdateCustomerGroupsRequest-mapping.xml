<?xml version="1.0"?>
<mapping>
 	<class name="uk.co.gazettes.services.tsoshop.castor.entities.customerservice.UpdateCustomerGroupsRequest">	  
	    <map-to xml="UpdateCustomerGroups" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos" />	      
	    <field name="customerId" type="string" >
	      	<bind-xml name="customerId" node="element"/>
	    </field>
	    <field name="customerGroups" type="uk.co.gazettes.services.tsoshop.castor.entities.customerservice.CustomerGroups" >
	      	<bind-xml name="customerGroups" node="element" />
	    </field>
	</class>
	
	<class name="uk.co.gazettes.services.tsoshop.castor.entities.customerservice.CustomerGroups">	  
	    <map-to xml="customerGroups" ns-uri="http://www.tsoshop.co.uk/" ns-prefix="tsos" />	      
	    <field name="groupIds" type="int" collection="arraylist" >
	      	<bind-xml name="arr:int" node="element" xmlns:arr="http://schemas.microsoft.com/2003/10/Serialization/Arrays" QName-prefix="arr"/>
	    </field>
	</class>
</mapping>