<!ELEMENT gazette (control, notice+)>
<!ATTLIST gazette
	type (london | edinburgh | belfast) #REQUIRED
>
<!ELEMENT control ((issue, date, isbn) | (issue, date, page, isbn))>
<!ELEMENT issue (#PCDATA)>
<!ELEMENT date (weekday, day, month, year)>
<!ELEMENT weekday (#PCDATA)>
<!ELEMENT day (#PCDATA)>
<!ELEMENT month (#PCDATA)>
<!ELEMENT year (#PCDATA)>
<!ELEMENT isbn (#PCDATA)>
<!--ELEMENT notice (((head3 | head4 | head5 | text | t | bt | r | c)+, number?) | (legalpre, legalhd, legal+))-->
<!ELEMENT notice (#PCDATA  | hw1 | hw2 | hw3 | hw4 | head3 | head4 | head5 | text | t | bt | r | c | number | legalpre | legalhd | legal)*>
<!ATTLIST notice
	code CDATA #REQUIRED
>
<!ELEMENT head3 (#PCDATA | hw1 | hw2 | hw3 | hw4)*>
<!ELEMENT head4 (#PCDATA | hw1 | hw2 | hw3 | hw4)*>
<!ELEMENT head5 (#PCDATA | hw1 | hw2 | hw3 | hw4 | COMPANYNA | X2ND_COMP | X3RD_COMP | X4TH_COMP | X5TH_COMP | X6TH_COMP | X7TH_COMP)*>
<!ELEMENT text (#PCDATA | tradingas | img | IMG | CENTER | REGISTERE | NATUREOFB | TRADECLAS | ADMINISTR | OFFICEHOL | OFFICE_HO | ADMIN_REC | TRADE_CLA | DATE_OF_A | NAME_OF_P | PREVIOUS_ | PLACE_OF_ | LIQUIDATO | MEETINGDA | MEETINGTI | COURTNAME | HEARINGNU | TRADING_N | TRADINGAS | receiver | name | actsection | meetingwhere | meetingdate | meetingtime | practioner | sig | t | hw1 | hw2 | hw3 | hw4 | legal)*>
<!ELEMENT CENTER (IMG | img)>
<!ELEMENT img EMPTY>
<!ATTLIST img
	src CDATA #IMPLIED
	filename CDATA #IMPLIED
	SRC CDATA #IMPLIED
>
<!ELEMENT IMG EMPTY>
<!ATTLIST IMG
	SRC CDATA #REQUIRED
>
<!ELEMENT tradingas (#PCDATA | hw1 | hw2 | hw3 | hw4)*>
<!ELEMENT receiver (#PCDATA | hw1 | hw2 | hw3 | hw4)*>
<!ELEMENT name (#PCDATA | hw1 | hw2 | hw3 | hw4)*>
<!ELEMENT actsection (#PCDATA | hw1 | hw2 | hw3 | hw4)*>
<!ELEMENT meetingwhere (#PCDATA | hw1 | hw2 | hw3 | hw4)*>
<!ELEMENT meetingdate (#PCDATA | hw1 | hw2 | hw3 | hw4)*>
<!ELEMENT meetingtime (#PCDATA | hw1 | hw2 | hw3 | hw4)*>
<!ELEMENT practioner (#PCDATA | hw1 | hw2 | hw3 | hw4)*>
<!ELEMENT sig (#PCDATA | signature | img | advertdate | advertnumber | hw1 | hw2 | hw3 | hw4 | SIGNATOR | OFFICEHOL | OFFICE_HO | ADMIN_REC)*>
<!ELEMENT signature (#PCDATA | hw1 | hw2 | hw3 | hw4)*>
<!ELEMENT advertdate (#PCDATA)>
<!ELEMENT advertnumber (#PCDATA)>
<!ELEMENT t (bt)>
<!ATTLIST t
	cols CDATA #REQUIRED
>
<!ELEMENT bt (#PCDATA | hw1 | hw2 | hw3 | hw4 | r)*>
<!ELEMENT r (#PCDATA | hw1 | hw2 | hw3 | hw4 | c)*>
<!ELEMENT c (#PCDATA | hw1 | hw2 | hw3 | hw4 | text)*>
<!ATTLIST c
	hstrad CDATA #IMPLIED
	vstrad CDATA #IMPLIED
>
<!ELEMENT page (#PCDATA)>
<!ELEMENT hw1 (#PCDATA | r | c)*>
<!ELEMENT hw2 (#PCDATA | r | c | SIGNATOR | SIGNATOR_)*>
<!ELEMENT hw3 (#PCDATA | r | c)*>
<!ELEMENT hw4 (#PCDATA | r | c)*>
<!ELEMENT COMPANYNA (#PCDATA)>
<!ELEMENT REGISTERE (#PCDATA)>
<!ELEMENT NATUREOFB (#PCDATA)>
<!ELEMENT TRADECLAS (#PCDATA)>
<!ELEMENT SIGNATOR (#PCDATA)>
<!ELEMENT SIGNATOR_ (#PCDATA)>
<!ELEMENT ADMINISTR (#PCDATA)>
<!ELEMENT OFFICEHOL (#PCDATA)>
<!ELEMENT OFFICE_HO (#PCDATA)>
<!ELEMENT ADMIN_REC (#PCDATA)>
<!ELEMENT TRADE_CLA (#PCDATA)>
<!ELEMENT DATE_OF_A (#PCDATA)>
<!ELEMENT NAME_OF_P (#PCDATA)>
<!ELEMENT PREVIOUS_ (#PCDATA)>
<!ELEMENT PLACE_OF_ (#PCDATA)>
<!ELEMENT LIQUIDATO (#PCDATA)>
